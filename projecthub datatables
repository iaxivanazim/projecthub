database projecthub:

	table1 projects:
		fields:
			1) proj_id
			2) proj_title
			3) proj_desc
			4) proj_imgs
			5) proj_vids
			6) team_name
			7) college_name
			
	table2 courses:
		fields:
			1) course_id
			2) course_title
			3) course_desc
			4) course_video
			5) course_proj_id (fk)   
			
	table3 teams:
		fields:
			1) team_id
			2) team_name
			3) team_members
			4) team_details
			5) team_proj_id (fk)